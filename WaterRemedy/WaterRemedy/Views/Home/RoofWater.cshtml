    <section>
        <div class="container" id="threePic">
            <h2 style="text-align:center; color:black; font-size:50px; font-weight:600">
                What it use for
            </h2>
            <div class="row" style="margin-top:50px">
                <div class="c1 col-lg-4 col-md-4 filter-app">
                    <img src="~/img/ggarden.png" class="img-fluid" id="ggarden" style="float:right" />
