                    <div class="words" style="margin-left:-125px">LAUNDRY</div>
                </div>
            </div>
        </div>
    </section>

    <section>
        <div class="container" id="team3">
            <h2 style="text-align:center; color:black; font-size:50px; font-weight:600">
                How much can save
            </h2>
            <div class="row" style="margin-top:50px">
                <div class="c1 col-lg-3 col-md-3 filter-app">
                    <label for="exampleForm2" style="font-weight:bold">Area(m^2)</label>
                    <input type="text" id="area" class="form-control">
                </div>
                <div class="c1 col-lg-3 col-md-3 filter-app">
                    <label for="exampleForm2" style="font-weight:bold">Rainfall(mm)</label>
                    <input type="text" id="rainfall" class="form-control">
                </div>
                <div class="c1 col-lg-3 col-md-3 filter-app">
                    <label for="exampleForm2" style="font-weight:bold">Total can be saved(L)</label>
                    <input type="text" id="storage" class="form-control">
                </div>
                <div class="c1 col-lg-3 col-md-3 filter-app">
                    <label for="exampleForm2" style="font-weight:bold">How much money you can save($)</label>
                    <input type="text" id="money" class="form-control">
                </div>
            </div>
        </div>
    </section>

